{
	"name": "devspace/logconnections",
	"type": "phpbb-extension",
	"description": "This extension will log user connections, failed logins and new users.",
	"version": "3.3.0",
	"time": "2022-10-29",
		"keywords": [
		"phpbb",
		"extension",
		"log",
		"connection",
		"user"
	],
	"license": "GPL-2.0-only",
	"authors": [{
	 	"name": "devspace",
		"role": "Maintainer"
    },
	{
		"name": "David Wood (david63)",
		"role": "Developer"
	}],
	"require": {
        "php": ">=5.3.3",
        "composer/installers": "~1.0"
    },
	"extra": {
		"display-name": "[devspace] Log user connections",
		"soft-require": {
			"phpbb/phpbb": ">=3.3.0,<4.0.0@dev"
      },
		"version-check": {
        	"host": "version.phpbbdev.space",
        	"directory": "/versions/devspace",
        	"filename": "log_connections_version_file.json",
			"ssl": true
    	}
	}
}
